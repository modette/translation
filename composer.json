{
	"name": "modette/translation",
	"description": "Fast, simple and scalable translation solution",
	"keywords": [
		"modette",
		"nette",
		"neon",
		"latte",
		"translation"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/modette/translation",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "^7.2",
		"modette/exceptions": "^1.0.0"
	},
	"require-dev": {
		"latte/latte": "^2.5.0",
		"nette/application": "^3.0.0",
		"nette/caching": "^3.0.0",
		"nette/di": "^3.0.0",
		"ninjify/qa": "^0.8.0",
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan-deprecation-rules": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-shim": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.4",
		"tracy/tracy": "^2.6.0"
	},
	"autoload": {
		"psr-4": {
			"Modette\\Translation\\": "src"
		},
		"files": [
			"src/shortcut.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\Translation\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
