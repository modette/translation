{
	"name": "modette/translation",
	"description": "Fast, simple and scalable translation solution",
	"keywords": [
		"modette",
		"nette",
	  	"symfony",
		"neon",
		"translation"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/modette/translation",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "^7.2.0",
		"modette/exceptions": "^1.0.0",
		"psr/log": "^1.1"
	},
	"require-dev": {
		"ext-intl": "*",
		"contributte/di": "^0.4.1",
		"latte/latte": "^2.5.0",
		"nette/application": "^3.0.0",
		"nette/caching": "^3.0.0",
		"ninjify/qa": "^0.10.0",
		"pepakriz/phpstan-exception-rules": "^0.10.1",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^8.5.0",
		"symfony/polyfill-intl-messageformatter": "^1.10.0",
		"symfony/translation-contracts": "^2.0.0",
		"tracy/tracy": "^2.6.0|^2.7.0"
	},
	"autoload": {
		"psr-4": {
			"Modette\\Translation\\": "src"
		},
		"files": [
			"src/shortcut.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\Translation\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
